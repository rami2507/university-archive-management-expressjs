extends base
block extraHead 
  link(rel='stylesheet', href='/css/archiveDemandes.css')
block content
  if archiveDemands.length === 0
    main
    section.no-results
      h2 No Archive Demands At The Moment
      a.btn.btn--primary(href="/manage-demands") Back to Demand Management
  else
    table
      thead
        th Nom D'utilisateur
        th Role
        th Actions
      tbody
        each demand in archiveDemands
          tr
            td= demand.utilisateur.nomdutilisateur
            td= demand.utilisateur.typedecompte
            td
              a(href=`/manage-demands/archive/${demand._id}`, target="_blank") View Demand
